{
  "name": "png-to-ico",
  "version": "1.0.1",
  "description": "convert png to windows ico format",
  "main": "index.js",
  "engines": {
    "node": ">=5"
  },
  "bin": {
    "png-to-ico": "bin/cli.js"
  },
  "files": [
    "bin/*",
		"*.md",
		"index.d.ts"
  ],
	"typings": "index.d.ts",
  "scripts": {
    "test": "xo && mocha",
		"mocha": "mocha",
		"lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steambap/png-to-ico.git"
  },
  "keywords": [
    "png",
    "ico",
    "fast",
    "convert"
  ],
  "author": "Weilin Shi <934587911@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steambap/png-to-ico/issues"
  },
  "homepage": "https://github.com/steambap/png-to-ico#readme",
  "dependencies": {
    "jimp": "^0.2.27",
    "meow": "^3.7.0"
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "xo": "^0.17.0"
  },
	"xo": {
		"esnext": true,
		"envs": ["mocha"],
    "rules": {
      "linebreak-style": ["error", "windows"]
    }
	}
}
